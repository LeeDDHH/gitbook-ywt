{
  "language": "ja",
  "title": "Kamo's YWT",
  "description": "カモの毎日のYWTをまとめるGitBook",
  "src": "docs",
  "plugins": [
    "expand-active-chapter",
    "collapsible-chapters",
    "navigator",
    "hide-published-with",
    "anchors",
    "numbered-headings",
    "hints",
    "advanced-emoji",
    "todo"
  ],
  "pluginsConfig":
  {
    "sharing":
    {
      "facebook": false,
      "twitter": true,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": [
          "facebook", "google", "twitter", "weibo", "instapaper"
      ]
    }
  },
  "styles": {
    "website": "./docs/css/index.css"
  }
}
